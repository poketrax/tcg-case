    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.6.0",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "storybook": "^6.5.16"
